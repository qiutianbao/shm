input.regular-checkbox01,label{ margin-top:10px; float:left;}
.regular-checkbox01 {
	display: none;
}

.regular-checkbox01 + label {
	background-color: #e5e5e5;
	border: 1px solid #dedede;
	width:15px;
	height:15px;
	display: inline-block;
	position: relative;
}

.regular-checkbox01 + label:active, .regular-checkbox01:checked + label:active {

}

.regular-checkbox01:checked + label {
	background-color: #efefef;
	border: 1px solid #dedede;
	background-image:url(../images/img01.png);
	background-repeat:no-repeat;
	background-size:15px 15px;

	color: #f27472;
}

.regular-checkbox01:checked + label:after {
	position: absolute;
	top: 0px;
	left: 3px;
	color: #fff;
}


