}
table {
    border-collapse: collapse;
    border-spacing: 0;
}


select,input[type='text'],input[type='password'] {
	width:180px;
}
input[type='button'],button {
	color: #44d;
	cursor: pointer;
}

textarea{
	width:300px;
	height:60px;
}
